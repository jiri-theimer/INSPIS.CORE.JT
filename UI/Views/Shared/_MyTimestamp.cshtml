@model BO.BaseBO
@inject BL.Factory _f

@if (Model.pid > 0)
{
    <hr />
    <div>
        <small>@_f.tra("Záznam založen"):</small>
        <small class="text-info">@Model.UserInsert @Model.DateInsert</small>


        @if (Model.DateUpdate > Model.DateInsert)
        {
            <small style="margin-left:20px;">@_f.tra("Poslední aktualizace"):</small>
            <small class="text-info">@Model.UserUpdate @Model.DateUpdate</small>

        }
    </div>
}
